<ul class="team-form">
    <% data.forEach(event => { %>
        
        <li>
            <span><%= event.dateStringShort %></span>
            <div class="team-wrapper">
                <span class="<%= event.intHomeScore > event.intAwayScore ? 'win' : 'loss' %>"><%= event.intHomeScore %></span>
                
                <picture>
                    <source width="35" height="auto" srcset="<%= event.homeTeamData.strTeamBadgeWebp ?? event.homeTeamData.strTeamBadge %>" type="image/webp">
                    <img width="28" height="28" src="<%= event.homeTeamData.strTeamBadge %>" alt="<%= event.homeTeamData.strTeam %> logo">
                </picture>

                <h3><%= event.homeTeamData.strTeam %></h3>

            </div>
    
            <div class="team-wrapper">
                <span class="<%= event.intAwayScore > event.intHomeScore ? 'win' : 'loss' %>"><%= event.intAwayScore %></span>
                
                <picture>
                    <source width="35" height="auto" srcset="<%= event.awayTeamData.strTeamBadgeWebp ?? event.awayTeamData.strTeamBadge %>" type="image/webp">
                    <img width="28" height="28" src="<%= event.awayTeamData.strTeamBadge %>" alt="<%= event.awayTeamData.strTeam %> logo">
                </picture>

                <h3><%= event.awayTeamData.strTeam %></h3>

            </div>
    
        </li>
    <% }) %>
</ul>