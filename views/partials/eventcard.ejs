<% if (isList) { %>
 <li class="event-card">
<% } else { %>
<div class="event-card<%= typeof fixedItem == 'undefined' ? '' : (fixedItem ? ' fixedItem' : '') %>">
<% } %>
    <div class="team-wrapper">
        <img src="<%= data.homeTeamData.strTeamBadge %>" alt="<%= data.strHomeTeam %> logo">
        <h3><%= data.strHomeTeam %></h3>
    </div>

    <div class="game-info">
        <% if (data.strLeague != 'English Womens Super League') { %>
            <span class="game-info__league"><%= data.strLeague %></span>
        <% } %>
        <span class="game-info__time"><%= data.localTime %></span>
        <span class="game-info__date"><%= !isList ? `This ${data.dateStringShort.toLowerCase()}` : data.dateStringShort %></span>
    </div>

    <div class="team-wrapper">
        <img src="<%= data.awayTeamData.strTeamBadge%>" alt="<%= data.strAwayTeam %> logo">
        <h3><%= data.strAwayTeam %></h3>
    </div>

    <img src="/static/wsl-logo.png" alt="WFUT logo">
    
<% if (isList) { %>
</li>
<% } else { %>
</div>
<% } %>