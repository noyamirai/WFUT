<% for(let squadPos in data) { %>
    <h3><%= squadPos %></h3>
    <ul>
        <% data[squadPos].forEach((player, i) => { %>
            <li class="player__item">
                <picture>
                    <img 
                    src="<%= player.strCutout ? player.strCutout : player.strThumb %>" 
                    data-src="<%= player.strCutout ? player.strCutout : player.strThumb %>" 
                    data-player-image 
                    loading="lazy"
                    class="hidden"
                    alt="<%= player.strPlayer %> cutout photo">
                </picture>
        
                <h3><%= player.strPlayer %></h3>
            </li>
        <% }) %>
    </ul>
<% } %>